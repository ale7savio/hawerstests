spring.application.name=shopify-orders-tests

http.port=8080

server.port=8443

# The format used for the keystore. It could be set to JKS in case it is a JKS file
server.ssl.key-store-type=PKCS12
# The path to the keystore containing the certificate
server.ssl.key-store=classpath:keystore/alejandroms.p12
# The password used to generate the certificate
server.ssl.key-store-password=Alejandro.123
# The alias mapped to the certificate
server.ssl.key-alias=alejandroms

security.require-ssl=true

#trust store location
trust.store=classpath:keystore/alejandroms.p12
#trust store password
trust.store.password=Alejandro.123